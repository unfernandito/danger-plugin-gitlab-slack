{
  "name": "danger-plugin-gitlab-slack",
  "description": "Report to Slack the result of Danger",
  "author": {
    "name": "unfernandito",
    "email": "lfbu.112@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unfernandito/danger-plugin-gitlab-slack.git"
  },
  "bugs": {
    "url": "https://github.com/unfernandito/danger-plugin-gitlab-slack/issues"
  },
  "homepage": "https://github.com/unfernandito/danger-plugin-gitlab-slack#readme",
  "keywords": [
    "danger",
    "danger-plugin",
    "slack"
  ],
  "version": "2.0.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged && npm run test && npm run build",
    "build": "tsc",
    "test": "jest",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "prepack": "npm run build",
    "semantic-release": "semantic-release",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --parser typescript --no-semi --trailing-comma es5 --write --print-width 120",
    "prettier-project": "npm run prettier-write -- 'src/**/*.{ts,tsx}'",
    "lint": "eslint src/ --ext ts",
    "prepare": "is-ci || husky install"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@slack/webhook": "6.0.0",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "commitizen": "^4.2.4",
    "danger": "10.6.4",
    "eslint": "7.29.0",
    "husky": "6.0.0",
    "is-ci": "^3.0.0",
    "jest": "27.0.5",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "semantic-release": "^17.4.4",
    "ts-jest": "27.0.3",
    "typescript": "4.3.4"
  },
  "optionalDependencies": {
    "esdoc": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint -- --fix",
      "npm run prettier-write --"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(.test)\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {},
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
